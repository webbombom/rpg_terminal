loja_fruta={
    'maca':2.40,
    'banana':3.60,
    'uva':7.60

}
preco_uva = loja_fruta.get('uva',{})
print('uva', preco_uva)


loja_fruta.update({'uva':8.90,'tomate': 10})
print(loja_fruta, '#tomate adicionado')

for chave, valor in loja_fruta.items():
  print(f'key:{chave}: value:{valor}')MIT License